
          <h3 id="print"><span class="col-sm-6">Books, media &amp; more</span>
            <% if @symphony_count>0 %>
              <a href="/symphony?q=<%= @query %>" class="col-sm-6 see-all-results"><%= number_with_delimiter(@symphony_count) %> results</a>
            <% end %>
          </h3>  
           <% if @symphony_count == 0 %>
              <p class="no-results">No material was found<p>
            <% end %>   
          <ul>
            <% i = 1%>
            <% @symphony.take(@local_symphony_limit).each do |id,symphony| %>
            <li>
              <div class="row no-margin">
                <div class="col-sm-12">
                  <span class="number-result-list"><%= i %>.</span> <a href="/catalog/<%= id %>"> <%= symphony[:title] %>
                  <% if symphony[:subtitle] %>: <%= symphony[:subtitle]%><% end %>
                  <% if symphony[:edition] %>, <%= symphony[:edition] %><% end %></em></a>
                  <% if symphony[:year] %>
                    <span class="date"><%= "(#{symphony[:year].first})" %></span>
                  <% end %>
                  <% if symphony[:format] and symphony[:format].first != "Unknown" %>
                    <span class="date"><%= symphony[:format].first %></span>
                  <% end %>
                  <% if symphony[:electronic].first == "Online" %>
                    <span class="date">(<%= symphony[:electronic].first %>)</span>
                  <% end %>
                  <% if symphony[:author] %>
                    <span class="author-result-list"><%= symphony[:author] %>
                  <% end %>
                  <% if symphony[:ual] %>
                    <img alt="licensed for  U of A only" title="licensed for  U of A only" src="//s3.amazonaws.com/libapps/sites/165/icons/6665/ualib-logo.png" style="height:8%;width:8%"/> </span>
                  <% end %>
                </div>
              
            </li>
            <% i+=1 %>
            <% end %>
          </ul>
          <div class="results-bottom">
            <% if @symphony_count>0 %>
              <a href="/symphony?q=<%= @query %>" class="col-sm-12">View all books, media &amp; more</a>
            <% end %>
          </div>
        </div> 
